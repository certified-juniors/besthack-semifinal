<h2>index page</h2>
<div class="main_stock">
    <p id="name_of_main_stock">{{mainstock.symbol}}</p>
    <p id="price_of_main_stock">{{mainstock.last_price}}</p>
    <div id="graph_of_main_stock">
        <canvas id="myChart" width="400" height="400"></canvas>
</div>
<div class="container">
    {{#each stocks}}
<div class="row">
    <div class="col-8">
        <div class="card">
            <div class="card-header">
                <h3>{{this.symbol}}</h3>
            </div>
            <div class="card-body">
                <h5 class="card-title">{{this.last_price}}</h5>
                <p class="card-text">{{this.name}}</p>
            </div>
        </div>
    </div>
</div>
{{/each}}
</div>